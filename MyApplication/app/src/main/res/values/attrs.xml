<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="auto_loop_background_text">
        <attr name="text_colors" format="reference"/>
        <attr name="text_backgrounds" format="reference"/>
    </declare-styleable>


    <declare-styleable name="auto_loop_background_img">
        <attr name="img_backgrounds" format="reference"/>
    </declare-styleable>

    <!-- 有边框和背景的View -->
    <declare-styleable name="BorderTextView">
        <attr name="strokeWidth" format="dimension"/>
        <attr name="cornerRadius" format="dimension"/>
        <attr name="borderStrokeColor" format="color"/>
        <attr name="enableBackColor" format="color" />
        <attr name="contentBackColor" format="color"/>
        <attr name="contentPressedColor" format="color"/>
        <attr name="followTextColor" format="boolean"/>
    </declare-styleable>


</resources>